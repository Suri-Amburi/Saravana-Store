<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:t="sap.ui.table" >                                                                                                                                                                       
<Dialog id="idDialod1" showHeader="true" contentWidth= "550px"                                                                                                                                                                                                 
					contentHeight="300px" title="Input Details" class="Dialogclass" resizable="false">                                                                                                                                                                        
	<content class="cl_dialog01">                                                                                                                                                                                                                                 
	<Toolbar>                                                                                                                                                                                                                                                     
	<Label text="SST Code:"/>                                                                                                                                                                                                                                     
	<Text text="{}" id="sstcodeid"/>                                                                                                                                                                                                                              
	<ToolbarSpacer/>                                                                                                                                                                                                                                              
	<Label text="Item Name:"/>                                                                                                                                                                                                                                    
	<Text text="{}" id="itemcode"/>                                                                                                                                                                                                                               
	<ToolbarSpacer/>                                                                                                                                                                                                                                              
	<Label text="Parent:"/>                                                                                                                                                                                                                                       
	<Text text="{}" id="parentnode"/>                                                                                                                                                                                                                             
	</Toolbar>                                                                                                                                                                                                                                                    
	<Toolbar>                                                                                                                                                                                                                                                     
	<Label text="Child Sst Code:"/>                                                                                                                                                                                                                               
	<Text text="{}" id="childcode"/>                                                                                                                                                                                                                              
	<ToolbarSpacer/>                                                                                                                                                                                                                                              
	<Label text="Rate:"/>                                                                                                                                                                                                                                         
	<Text text="{}" id="rate4321"/>                                                                                                                                                                                                                               
	<ToolbarSpacer/>&nbsp;                                                                                                                                                                                                                                        
	<Label text="Quantity:"/>                                                                                                                                                                                                                                     
	<Text text="{}" id="quanid"/>                                                                                                                                                                                                                                 
	</Toolbar>                                                                                                                                                                                                                                                    
<Toolbar class="cl_tlbr">                                                                                                                                                                                                                                      
<Label text= "Style" class="cl_label"/>                                                                                                                                                                                                                        
<ToolbarSpacer/>                                                                                                                                                                                                                                               
<Label text = "Color" class="cl_label"/>                                                                                                                                                                                                                       
<ToolbarSpacer/>                                                                                                                                                                                                                                               
<Label text = "Quantity" class="cl_label"/>                                                                                                                                                                                                                    
<ToolbarSpacer/>                                                                                                                                                                                                                                               
<Label text = "Action" class="cl_label"/>                                                                                                                                                                                                                      
</Toolbar>                                                                                                                                                                                                                                                     
<Toolbar>                                                                                                                                                                                                                                                      
<Input type="Text" id="styleget" enabled ="true"/>                                                                                                                                                                                                             
<ToolbarSpacer/>                                                                                                                                                                                                                                               
<Input type= "Text" id="colorget" enabled ="true"/>                                                                                                                                                                                                            
<ToolbarSpacer/>                                                                                                                                                                                                                                               
<Input  type="Number" id="quantityget" enabled ="true"/>                                                                                                                                                                                                       
<ToolbarSpacer/>                                                                                                                                                                                                                                               
<Button icon= "sap-icon://accept" id="popaccept" press="addit" enabled = "true"/>                                                                                                                                                                              
</Toolbar>                                                                                                                                                                                                                                                     
<t:Table rows="{/results}" id="tablesize23"  class="cl_table1" visible="false" selectionMode="None">                                                                                                                                                           
				<t:columns class="cl_sstL">                                                                                                                                                                                                                                
				<t:Column hAlign="Center">                                                                                                                                                                                                                                 
				<Label text="Style" class="cl_pcc"/>                                                                                                                                                                                                                       
				<t:template>                                                                                                                                                                                                                                               
			     <Text text="{Style}" />                                                                                                                                                                                                                                
				</t:template>                                                                                                                                                                                                                                              
				</t:Column>                                                                                                                                                                                                                                                
				<t:Column  hAlign="Center" class="cl_pcc">                                                                                                                                                                                                                 
				<Label  text="Color" class="cl_pcc"/>                                                                                                                                                                                                                      
				<t:template>                                                                                                                                                                                                                                               
				<Text text="{color}" />                                                                                                                                                                                                                                    
			    </t:template>                                                                                                                                                                                                                                           
				</t:Column>                                                                                                                                                                                                                                                
			    <t:Column  hAlign="Center">                                                                                                                                                                                                                             
				<Label  text="Quantitiy" class="cl_pcc"/>                                                                                                                                                                                                                  
				<t:template>                                                                                                                                                                                                                                               
				<Text text="{Quantity}"/>                                                                                                                                                                                                                                  
				</t:template>                                                                                                                                                                                                                                              
				</t:Column>                                                                                                                                                                                                                                                
				<!--<t:Column  hAlign="Center">-->                                                                                                                                                                                                                         
				<!--<Label  text=""/>-->                                                                                                                                                                                                                                   
				<!--<t:template>-->                                                                                                                                                                                                                                        
				<!--<Button icon="sap-icon://delete" press="popdelete"/>-->                                                                                                                                                                                                
				<!--</t:template>-->                                                                                                                                                                                                                                       
				<!--</t:Column>-->                                                                                                                                                                                                                                         
				</t:columns>                                                                                                                                                                                                                                               
				</t:Table>                                                                                                                                                                                                                                                 
</content>                                                                                                                                                                                                                                                     
<beginButton>                                                                                                                                                                                                                                                  
<Button class="cl_dialog" id="onenter" icon="sap-icon://accept"   press="onenter"/>                                                                                                                                                                            
</beginButton>                                                                                                                                                                                                                                                 
<endButton >                                                                                                                                                                                                                                                   
<Button class="cl_dialog" id="onclose" icon="sap-icon://decline"  press="onCancel"/>                                                                                                                                                                           
</endButton>                                                                                                                                                                                                                                                   
</Dialog>                                                                                                                                                                                                                                                      
</core:FragmentDefinition>                                                                                                                                                                                                                                     