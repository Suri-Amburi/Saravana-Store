<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:t="sap.ui.table">                                                                                                                                                                        
	<Dialog id="sizeorset" showHeader="true"  title= "Size Details" class="Dialogclass" resizable="false">                                                                                                                                                        
		<content>                                                                                                                                                                                                                                                    
			<Toolbar>                                                                                                                                                                                                                                                   
				<SearchField id="sizesearch"  liveChange="onsearch" tooltip="Search" width="30%"></SearchField>                                                                                                                                                            
			<ToolbarSpacer/>                                                                                                                                                                                                                                            
			<!--<Input type="Number" id="quantityinput" width="30%" showValueHelp="false"	valueHelpRequest="handleValueHelp" submit="true" change="submit"/>-->                                                                                                         
			                                                                                                                                                                                                                                                            
			</Toolbar>                                                                                                                                                                                                                                                  
			<t:Table rows="{/results}" id="tablesize23" class="cl_table1"   visible="true" visibleRowCount="1" selectionMode="None">                                                                                                                                    
				<t:columns class="cl_sstL">                                                                                                                                                                                                                                
				<t:Column  hAlign="Center" class="cl_pcc">                                                                                                                                                                                                                 
				<Label  text="Size" class="cl_pcc"/>                                                                                                                                                                                                                       
				<t:template>                                                                                                                                                                                                                                               
				<Text  text="{Size1}" />                                                                                                                                                                                                                                   
			    </t:template>                                                                                                                                                                                                                                           
				</t:Column>                                                                                                                                                                                                                                                
			    <t:Column  hAlign="Center">                                                                                                                                                                                                                             
				<Label  text="Quantitiy" class="cl_pcc"/>                                                                                                                                                                                                                  
				<t:template>                                                                                                                                                                                                                                               
				<Input width= "30%"  type="Number" value="{Quantity}"/>                                                                                                                                                                                                    
				</t:template>                                                                                                                                                                                                                                              
				</t:Column>                                                                                                                                                                                                                                                
				</t:columns>                                                                                                                                                                                                                                               
				</t:Table>                                                                                                                                                                                                                                                 
            </content>                                                                                                                                                                                                                                         
	    <beginButton>                                                                                                                                                                                                                                             
			<Button class="cl_dialog" id="idbuttondialog"  text="Accept" press="Onaccept"/>                                                                                                                                                                             
		</beginButton>                                                                                                                                                                                                                                               
		<endButton >                                                                                                                                                                                                                                                 
			<Button class="cl_dialog" id="idbuttondialog2"  text="Close" press="onCancel"/>                                                                                                                                                                             
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     